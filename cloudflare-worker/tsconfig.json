{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021"
        ],
        "module": "ES2022",
        "moduleResolution": "node",
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ]
}